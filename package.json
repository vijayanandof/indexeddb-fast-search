{
  "name": "indexeddb-fast-search",
  "version": "0.1.0",
  "description": "A lightweight, Promise-based, and fast IndexedDB wrapper focused on efficient searching and ease of use",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "examples"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "docs": "typedoc",
    "start:example": "npm run build && http-server . -o examples/web/",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run docs",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/indexeddb-fast-search.git"
  },
  "keywords": [
    "indexeddb",
    "database",
    "search",
    "typescript",
    "browser",
    "promise",
    "fast",
    "query",
    "index"
  ],
  "author": {
    "name": "Vijayanand",
    "email": "vijayanandof@gmail.com",
    "url": "https://github.com/vijayanandof"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yourusername/indexeddb-fast-search/issues"
  },
  "homepage": "https://github.com/yourusername/indexeddb-fast-search#readme",
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.14.1",
    "fake-indexeddb": "^6.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "rollup": "^4.9.6",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.8",
    "typedoc-plugin-markdown": "^3.17.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
